{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "removeComments": true,
      "sourceMap": false,
  
      "outDir": "./dist",
      "rootDir": "",
      "resolveJsonModule": true,
  
      "skipLibCheck": true,
      "allowJs": true,
      // "checkJs": true,
      
      "strict": true,
    },
    "exclude": ["node_modules", "dist/*", "static"],
    "lib": ["es6"]
  }